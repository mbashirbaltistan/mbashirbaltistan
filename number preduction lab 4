        "from tensorflow.keras.layers import MaxPooling2D as MaxPool2D\n",
        "model = Sequential()\n",
        "#adding convolutional layer\n",
        "model.add(Conv2D(32, (3,3), activation='relu', input_shape=(28, 28, 1)))\n",
